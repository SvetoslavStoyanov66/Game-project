  achievementDiscription: "\u041C\u043E\u0440\u043A\u043E\u0432\u0438\u0442\u0435
    (\u043D\u0430\u0443\u0447\u043D\u043E \u043D\u0430\u0438\u043C\u0435\u043D\u043E\u0432\u0430\u043D\u0438\u0435:
    Daucus carota subsp. sativus) \u0441\u0430 \u043A\u043E\u0440\u0435\u043D\u043E\u043F\u043B\u043E\u0434\u043D\u0438
    \u0437\u0435\u043B\u0435\u043D\u0447\u0443\u0446\u0438, \u0448\u0438\u0440\u043E\u043A\u043E
    \u0440\u0430\u0437\u043F\u0440\u043E\u0441\u0442\u0440\u0430\u043D\u0435\u043D\u0438
    \u0438 \u0446\u0435\u043D\u0435\u043D\u0438 \u0437\u0430\u0440\u0430\u0434\u0438
    \u0442\u0435\u0445\u043D\u0438\u0442\u0435 \u0445\u0440\u0430\u043D\u0438\u0442\u0435\u043B\u043D\u0438
    \u043A\u0430\u0447\u0435\u0441\u0442\u0432\u0430 \u0438 \u0432\u043A\u0443\u0441.
    \u041F\u0440\u0438\u043D\u0430\u0434\u043B\u0435\u0436\u0430\u0442 \u043A\u044A\u043C
    \u0441\u0435\u043C\u0435\u0439\u0441\u0442\u0432\u043E Apiaceae (\u0438\u043B\u0438
    Umbelliferae), \u043A\u043E\u0435\u0442\u043E \u0441\u044A\u0449\u043E \u0432\u043A\u043B\u044E\u0447\u0432\u0430
    \u043A\u043E\u043F\u044A\u0440, \u043C\u0430\u0433\u0434\u0430\u043D\u043E\u0437,
    \u0438 \u0446\u0435\u043B\u0438\u043D\u0430. \u041C\u043E\u0440\u043A\u043E\u0432\u0438\u0442\u0435
    \u0438\u043C\u0430\u0442 \u0434\u044A\u043B\u0433\u0430 \u0438\u0441\u0442\u043E\u0440\u0438\u044F
    \u043D\u0430 \u043A\u0443\u043B\u0442\u0438\u0432\u0430\u0446\u0438\u044F, \u043A\u0430\u0442\u043E
    \u0441\u0435 \u0441\u043C\u044F\u0442\u0430, \u0447\u0435 \u043F\u044A\u0440\u0432\u043E\u043D\u0430\u0447\u0430\u043B\u043D\u043E
    \u0441\u0430 \u0431\u0438\u043B\u0438 \u043E\u0442\u0433\u043B\u0435\u0436\u0434\u0430\u043D\u0438
    \u0432 \u0410\u0444\u0433\u0430\u043D\u0438\u0441\u0442\u0430\u043D \u043F\u0440\u0435\u0434\u0438
    \u043F\u043E\u0432\u0435\u0447\u0435 \u043E\u0442 1000 \u0433\u043E\u0434\u0438\u043D\u0438,
    \u043F\u0440\u0435\u0434\u0438 \u0434\u0430 \u0441\u0435 \u0440\u0430\u0437\u043F\u0440\u043E\u0441\u0442\u0440\u0430\u043D\u044F\u0442
    \u0432 \u0410\u0440\u0430\u0431\u0441\u043A\u0438\u044F \u0441\u0432\u044F\u0442
    \u0438 \u0441\u043B\u0435\u0434 \u0442\u043E\u0432\u0430 \u0432 \u0415\u0432\u0440\u043E\u043F\u0430
    \u0438 \u043E\u0441\u0442\u0430\u043D\u0430\u043B\u0430\u0442\u0430 \u0447\u0430\u0441\u0442
    \u043E\u0442 \u0441\u0432\u0435\u0442\u0430.  \u041C\u043E\u0440\u043A\u043E\u0432\u0438\u0442\u0435
    \u0441\u0430 \u0438\u0437\u043A\u043B\u044E\u0447\u0438\u0442\u0435\u043B\u043D\u043E
    \u0431\u043E\u0433\u0430\u0442\u0438 \u043D\u0430 \u0431\u0435\u0442\u0430-\u043A\u0430\u0440\u043E\u0442\u0438\u043D,
    \u043F\u0440\u0435\u0434\u0448\u0435\u0441\u0442\u0432\u0435\u043D\u0438\u043A
    \u043D\u0430 \u0432\u0438\u0442\u0430\u043C\u0438\u043D \u0410, \u043A\u043E\u0439\u0442\u043E
    \u0435 \u0432\u0430\u0436\u0435\u043D \u0437\u0430 \u0437\u0434\u0440\u0430\u0432\u0435\u0442\u043E
    \u043D\u0430 \u043E\u0447\u0438\u0442\u0435, \u043A\u043E\u0436\u0430\u0442\u0430
    \u0438 \u0438\u043C\u0443\u043D\u043D\u0430\u0442\u0430 \u0441\u0438\u0441\u0442\u0435\u043C\u0430.
    \u0421\u044A\u0449\u043E \u0442\u0430\u043A\u0430 \u0441\u044A\u0434\u044A\u0440\u0436\u0430\u0442
    \u0432\u0438\u0442\u0430\u043C\u0438\u043D\u0438 \u043E\u0442 \u0433\u0440\u0443\u043F\u0430\u0442\u0430
    B, \u0432\u0438\u0442\u0430\u043C\u0438\u043D K \u0438 \u0432\u0438\u0442\u0430\u043C\u0438\u043D
    C, \u043A\u0430\u043A\u0442\u043E \u0438 \u043C\u0438\u043D\u0435\u0440\u0430\u043B\u0438
    \u043A\u0430\u0442\u043E \u043A\u0430\u043B\u0438\u0439 \u0438 \u043C\u0430\u043D\u0433\u0430\u043D.
    \u0422\u0435 \u043F\u0440\u0435\u0434\u043B\u0430\u0433\u0430\u0442 \u0438 \u0434\u0438\u0435\u0442\u0438\u0447\u043D\u0438
    \u0444\u0438\u0431\u0440\u0438, \u043A\u043E\u0438\u0442\u043E \u043F\u043E\u0434\u043F\u043E\u043C\u0430\u0433\u0430\u0442
    \u0445\u0440\u0430\u043D\u043E\u0441\u043C\u0438\u043B\u0430\u043D\u0435\u0442\u043E. 
    \u0421\u0432\u0435\u0436\u0438\u044F\u0442, \u0445\u0440\u0443\u043F\u043A\u0430\u0432
    \u0432\u043A\u0443\u0441 \u043D\u0430 \u043C\u043E\u0440\u043A\u043E\u0432\u0438\u0442\u0435
    \u0433\u0438 \u043F\u0440\u0430\u0432\u0438 \u043F\u043E\u043F\u0443\u043B\u044F\u0440\u0435\u043D
    \u0438\u0437\u0431\u043E\u0440 \u043A\u0430\u043A\u0442\u043E \u0437\u0430 \u0441\u0443\u0440\u043E\u0432\u0430
    \u043A\u043E\u043D\u0441\u0443\u043C\u0430\u0446\u0438\u044F, \u0442\u0430\u043A\u0430
    \u0438 \u0437\u0430 \u0433\u043E\u0442\u0432\u0435\u043D\u0435. \u0422\u0435
    \u043C\u043E\u0433\u0430\u0442 \u0434\u0430 \u0441\u0435 \u0434\u043E\u0431\u0430\u0432\u044F\u0442
    \u0432 \u0441\u0430\u043B\u0430\u0442\u0438, \u0441\u0443\u043F\u0438, \u044F\u0445\u043D\u0438\u0438,
    \u0434\u0430 \u0441\u0435 \u043F\u0435\u043A\u0430\u0442, \u0432\u0430\u0440\u044F\u0442,
    \u043F\u0440\u0430\u0432\u044F\u0442 \u043D\u0430 \u043F\u0430\u0440\u0430 \u0438\u043B\u0438
    \u0434\u0430 \u0441\u0435 \u0438\u0437\u043F\u043E\u043B\u0437\u0432\u0430\u0442
    \u0437\u0430 \u043F\u0440\u0438\u0433\u043E\u0442\u0432\u044F\u043D\u0435 \u043D\u0430
    \u0434\u0435\u0441\u0435\u0440\u0442\u0438 \u0438 \u0441\u043E\u043A\u043E\u0432\u0435.
    \u041C\u043E\u0440\u043A\u043E\u0432\u0438\u0442\u0435 \u043D\u0435 \u0441\u0430\u043C\u043E
    \u0434\u043E\u0431\u0430\u0432\u044F\u0442 \u0446\u0432\u044F\u0442 \u0438 \u0432\u043A\u0443\u0441
    \u043D\u0430 \u044F\u0441\u0442\u0438\u044F\u0442\u0430, \u043D\u043E \u0438
    \u043F\u043E\u0434\u043E\u0431\u0440\u044F\u0432\u0430\u0442 \u0445\u0440\u0430\u043D\u0438\u0442\u0435\u043B\u043D\u0438\u044F
    \u0438\u043C \u043F\u0440\u043E\u0444\u0438\u043B. "
  UnUnlockedThubnail: {fileID: 21300000, guid: ec162f03c7c2c5f4c81076c0ac85f5f3, type: 3}
